<p id="notice"><%= notice %></p>

<h1>Your Feed</h1>

<p>
    Memes of profiles you've subscribed to will appear here.
</p>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feed_memes.each do |meme| %>
      <tr>
        <td><span><%= meme.title %></span><%= image_tag meme.image %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New User Feed', new_user_feed_path %>
